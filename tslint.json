{
    "extends": [
        "tslint:recommended",
        "tslint-eslint-rules"
    ],
    "rules": {
        "interface-name": [
            true,
            "never-prefix"
        ],
        "max-classes-per-file": false,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "no-console": {
            "severity": "warning",
            "options": [
                "debug",
                "info",
                "log",
                "time",
                "timeEnd",
                "trace"
            ]
        },
        "no-string-literal": false,
        "object-literal-sort-keys": false,
        "only-arrow-functions": true,
        "semicolon": true,
        "quotemark": [
            true,
            "single",
            "jsx-single"
        ],
        "trailing-comma": [
            false
        ]
    }
}